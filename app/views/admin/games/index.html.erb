<p id="notice"><%= notice %></p>

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Url</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.title %></td>
        <td><%= game.description %></td>
        <td><%= game.url %></td>
        <td><%= game.image %></td>
        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Edit', edit_admin_game_path(game) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to 'New Game', new_admin_game_path %>